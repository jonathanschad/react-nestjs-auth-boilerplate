{
    "$schema": "https://json.schemastore.org/nest-cli",
    "collection": "@nestjs/schematics",
    "sourceRoot": "src",
    "compilerOptions": {
        "deleteOutDir": true,
        "assets": ["**/*.html", "**/*.json", "**/*.png", "**/*.jpg", "**/*.jpeg", "**/*.svg", "**/*.md"],
        "watchAssets": true
    }
}
