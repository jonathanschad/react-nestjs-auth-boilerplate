{
    "name": "react-nestjs-auth-boilerplate",
    "version": "0.5.0",
    "description": "",
    "scripts": {
        "build": "turbo build",
        "dev": "turbo dev",
        "dev:client": "turbo dev --filter=@boilerplate/client",
        "dev:server": "turbo dev --filter=@boilerplate/server",
        "lint": "turbo lint",
        "lint:fix": "turbo lint:fix",
        "format": "prettier --write \"**/*.{ts,tsx,md,js,json}\"",
        "postinstall": "pnpm generate:prisma",
        "generate:prisma": "pnpm --filter @boilerplate/prisma db:generate"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "packageManager": "pnpm@10.8.1",
    "engines": {
        "node": ">=22.4.0"
    },
    "devDependencies": {
        "turbo": "^2.5.2",
        "typescript": "^5.8.3",
        "prettier": "^3.5.3"
    }
}
