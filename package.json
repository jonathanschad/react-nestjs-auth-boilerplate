{
    "name": "react-nestjs-auth-darts",
    "version": "0.7.0",
    "description": "",
    "scripts": {
        "build": "turbo build",
        "dev": "turbo dev",
        "dev:client": "turbo dev --filter=@darts/client",
        "dev:server": "turbo dev --filter=@darts/server",
        "lint": "turbo lint",
        "lint:fix": "turbo lint:fix",
        "format:verify": "biome check",
        "format": "biome check --write",
        "tsc": "turbo tsc",
        "postinstall": "pnpm db:generate-client-and-types",
        "db:generate-client-and-types": "pnpm --filter @darts/prisma db:generate-client-and-types",
        "db:create-migration": "pnpm --filter @darts/prisma db:create-migration",
        "db:apply-migration": "pnpm --filter @darts/prisma db:apply-migration"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "packageManager": "pnpm@10.8.1",
    "engines": {
        "node": ">=22.4.0"
    },
    "devDependencies": {
        "@biomejs/biome": "catalog:",
        "turbo": "^2.5.2",
        "typescript": "catalog:"
    }
}
