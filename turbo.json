{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "dev": {
            "persistent": true,
            "cache": false,
            "dependsOn": ["^dev"],
            "inputs": ["src/**/*.ts", "package.json", "tsconfig.json"],
            "outputs": ["dist/**"],
            "env": ["NODE_ENV"]
        }
    }
}
